{
  "connection": "default",
  "collectionName": "categories",
  "info": {
    "name": "category",
    "description": "Model opisujący kategorie dla artykułów."
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "key": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "articles": {
      "collection": "article",
      "via": "category"
    }
  }
}